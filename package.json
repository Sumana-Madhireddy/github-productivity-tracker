{
  "name": "github-productivity-tracker",
  "displayName": "GitHub productivity tracker",
  "publisher": "GitHubProductivityTracker",
  "description": "A GitHub productivity VSCode extension that logs coding activity and generates contributions every 30 minutes by committing work summaries to a dedicated repository.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "github-productivity-tracker.authenticate",
        "title": "Authenticate with GitHub"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "querystring": "^0.2.1"
  }
}
